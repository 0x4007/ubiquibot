{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": ["ESNext"],
    "allowJs": false,
    "skipLibCheck": false,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "amd",
    "moduleResolution": "Node",
    "types": ["@cloudflare/workers-types"],
    "outFile": "./_worker.js"
  },
  "include": ["./_worker.ts"]
}
